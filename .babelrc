{
    "presets": [["env", { "modules": false }], "react"],
    "plugins": [
        [
            "babel-plugin-transform-react-jsx",
            {
                "pragma": "createElement"
            }
        ],
        [
            "babel-plugin-jsx-pragmatic",
            {
                "module": "forsteri/pragma",
                "import": "createElement"
            }
        ]
    ]
}
